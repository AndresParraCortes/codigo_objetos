      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "\n",
        "class Nodo:\n",
        "    def __init__(self, datos):\n",
        "        self.datos = datos\n",
        "        self.siguiente = None\n",
        "        self.anterior = None\n",
        "\n",
        "class ListaDoblementeEnlazada:\n",
        "    def __init__(self):\n",
        "        self.cabeza = None\n",
        "        self.cola = None\n",
        "\n",
        "    def insertar(self, datos):\n",
        "        nuevo_nodo = Nodo(datos)\n",
        "        if not self.cabeza:\n",
        "            self.cabeza = self.cola = nuevo_nodo\n",
        "        else:\n",
        "            nuevo_nodo.anterior = self.cola\n",
        "            self.cola.siguiente = nuevo_nodo\n",
        "            self.cola = nuevo_nodo\n",
        "\n",
        "    def mostrar_adelante_atras(self):\n",
        "        actual = self.cabeza\n",
        "        while actual:\n",
        "            print(actual.datos)\n",
        "            actual = actual.siguiente\n",
        "\n",
        "    def mostrar_atras_adelante(self):\n",
        "        actual = self.cola\n",
        "        while actual:\n",
        "            print(actual.datos)\n",
        "            actual = actual.anterior\n",
        "\n",
        "    def ordenar_por_campo(self, campo, ascendente=True):\n",
        "        nodos = []\n",
        "        actual = self.cabeza\n",
        "        while actual:\n",
        "            nodos.append(actual)\n",
        "            actual = actual.siguiente\n",
        "\n",
        "        # Verificar si el campo existe en los datos\n",
        "        if nodos and campo not in nodos[0].datos:\n",
        "            print(f\"El campo '{campo}' no existe en los datos. Campos disponibles: {list(nodos[0].datos.keys())}\")\n",
        "            return\n",
        "\n",
        "        nodos.sort(key=lambda x: x.datos.get(campo, ''), reverse=not ascendente)\n",
        "        self.cabeza = nodos[0]\n",
        "        self.cola = nodos[-1]\n",
        "        for i in range(len(nodos)):\n",
        "            if i > 0:\n",
        "                nodos[i].anterior = nodos[i-1]\n",
        "            if i < len(nodos) - 1:\n",
        "                nodos[i].siguiente = nodos[i+1]\n",
        "        nodos[0].anterior = None\n",
        "        nodos[-1].siguiente = None\n",
        "\n",
        "def cargar_datos(archivo):\n",
        "    df = pd.read_csv(archivo, encoding='latin1', delimiter=',')\n",
        "    lista = ListaDoblementeEnlazada()\n",
        "    for _, fila in df.iterrows():\n",
        "        lista.insertar(fila.to_dict())\n",
        "    return lista\n",
        "\n",
        "# Uso del programa\n",
        "archivo = 'xbc7-65j4.csv'\n",
        "lista_datos = cargar_datos(archivo)\n",
        "\n",
        "print(\"Mostrando los primeros 10 datos de adelante hacia atrás:\")\n",
        "contador = 0\n",
        "actual = lista_datos.cabeza\n",
        "while actual and contador < 10:\n",
        "    print(actual.datos)\n",
        "    actual = actual.siguiente\n",
        "    contador += 1\n",
        "\n",
        "print(\"\\nMostrando los primeros 10 datos de atrás hacia adelante:\")\n",
        "contador = 0\n",
        "actual = lista_datos.cola\n",
        "while actual and contador < 10:\n",
        "    print(actual.datos)\n",
        "    actual = actual.anterior\n",
        "    contador += 1\n",
        "\n",
        "# Intenta ordenar por 'codigo' si existe, si no, usa el primer campo disponible\n",
        "primer_nodo = lista_datos.cabeza\n",
        "if primer_nodo:\n",
        "    campos = list(primer_nodo.datos.keys())\n",
        "    campo_ordenamiento = 'codigo' if 'codigo' in campos else campos[0]\n",
        "    print(f\"\\nOrdenando los primeros 10 datos por '{campo_ordenamiento}' de forma ascendente:\")\n",
        "    lista_datos.ordenar_por_campo(campo_ordenamiento)\n",
        "    contador = 0\n",
        "    actual = lista_datos.cabeza\n",
        "    while actual and contador < 10:\n",
        "        print(actual.datos)\n",
        "        actual = actual.siguiente\n",
        "        contador += 1\n",
        "\n",
        "# Intenta ordenar por 'nombre' si existe, si no, usa el último campo disponible\n",
        "if primer_nodo:\n",
        "    campos = list(primer_nodo.datos.keys())\n",
        "    campo_ordenamiento = 'nombre' if 'nombre' in campos else campos[-1]\n",
        "    print(f\"\\nOrdenando todos los datos por '{campo_ordenamiento}' de forma descendente:\")\n",
        "    lista_datos.ordenar_por_campo(campo_ordenamiento, ascendente=False)\n",
        "    lista_datos.mostrar_adelante_atras()"
      ],
